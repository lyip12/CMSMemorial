        {
            "ID": 230,
            "Name": "Marsh Chapel and Howard Thurman",
            "type": "Feature", "geometry": { "type": "Point", "coordinates": [ -71.106401, 42.350563 ]},
            "Ownership": "",
            "AddressNum": 735,
            "Street": "Commonwealth Ave",
            "City": "Boston",

        {
            "ID": 424,
            "Name": "Bijou Theater/Amusement Center",
            "type": "Feature", "geometry": { "type": "Point", "coordinates": [ -71.062571, 42.354042 ]},
            "Ownership": "",
            "AddressNum": "543-547",
            "Street": "Washington Street",
            "City": "Boston",

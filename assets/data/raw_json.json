        {
            "ID": 402,
            "Name": "Federal Street Church",
            "type": "Feature", "geometry": { "type": "Point", "coordinates": [ -71.055941, 42.3550011 ]},
            "Ownership": "",
            "AddressNum": "",
            "Street": "",
            "City": "",

        {
            "ID": 420,
            "Name": "Chester Square: Anna Quincy Waterston, Harriet Boyd Hawes, the South End Historical Society, and Betty Gibson",
            "type": "Feature", "geometry": { "type": "Point", "coordinates": [ -71.079182, 42.338801 ]},

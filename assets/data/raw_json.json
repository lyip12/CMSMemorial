        {
            "ID": 403,
            "Name": "Franklin Place and Home of Judith Sargent Murray",
            "type": "Feature", "geometry": { "type": "Point", "coordinates": [ -71.058032, 42.3554394 ]},
            "Ownership": "",
            "AddressNum": "",

        {
            "ID": 409,
            "Name": "Harriet Tubman Square Statues: Emancipation by Meta Vaux Warrick Fuller and Step on Board by Fern Cunningham",
            "type": "Feature", "geometry": { "type": "Point", "coordinates": [ -71.077958, 42.3434753 ]},
            "Ownership": "",
